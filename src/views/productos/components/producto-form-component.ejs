<div class="row">
    <div class="col s12 m4">
        <br><br><br>
        <a href="#" class="grey-text text-darken-2 center"><img class="responsive-img"
                src="https://cdn-icons-png.flaticon.com/512/8512/8512378.png"></a>
    </div>

    <div class="col s12 m8">
        <br><br><br>
        <form id="formCliente" action="./register" method="post" class="col s15">
            <div class="col s12 m5">

                <div class="input-field col s12">
                    <input type="text" name="nombre_pro" id="nombre_pro" class="validate" required>
                    <label for="nombre_pro">Nombre</label>
                </div>

                <div class="input-field col s12">
                    <input type="text" name="descripcion_pro" id="descripcion_pro" class="validate" required>
                    <label for="descripcion_pro">Descripcion producto</label>
                </div>

                <div class="input-field col s12">
                    <input type="text" name="precio_pro" id="precio_pro" class="validate" required>
                    <label for="precio_pro">Precio </label>
                </div>
            </div>

            <div class="file-field input-field">
                <div class="btn">
                    <span>Img</span>
                    <input type="file" multiple>
                </div>
                <div class="file-path-wrapper">
                    <input class="file-path validate" type="text" placeholder="Subir imagen del producto">
                </div>
            </div>
            <div class="col s18 m4">
                <div class="section">
                    <div class="input-field col s12">
                        <input type="text" name="categoria_pro" id="categoria_pro" required>
                        <label for="categoria_pro">Categoria producto</label>
                    </div>

                    <div class="input-field col s12">
                        <input type="text" name="estatus_pro" id="estatus_pro" required>
                        <label for="estatus_pro">Estatus producto</label>
                    </div>
                </div>

                <div class="input-field col s14">
                    <button type="submit" id="btnGuardar" class="botonDentro">
                        <span id="span-1">Guardar</span>
                    </button>
                </div>
            </div>
            <br>
        </form>
    </div>

</div>


</body>